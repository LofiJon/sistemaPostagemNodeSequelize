
<div class="container">
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6 mt-5">
            <h1 class="p-5"> Lista de postagens</h1>
           
                {{#each posts}}
                <ul class="list-group">
                    <li class="list-group-item mt-2">
                <span>{{dataValues.createdAt}}</span>
                    <h1>{{dataValues.titulo}}</h1>
                    <p>{{dataValues.conteudo}}</p>
                                      <a href="/deletar/{{dataValues.id}}" class="btn btn-danger">Deletar</a>
                    </li>
                                
                </ul>
       
                {{/each}}
        </div>
          <div class="col-md-3"></div>
    </div>
</div>